{
  "name": "kaishiyoku/laravel-menu",
  "description": "Create menus for Laravel with css-class highlighting support",
  "license": "MIT",
  "authors": [
    {
      "name": "Kaishiyoku",
      "email": "dev@andreas-wiedel.de"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": "^6.0|^7.0",
    "illuminate/contracts": "^6.0|^7.0",
    "illuminate/routing": "^6.0|^7.0",
    "illuminate/view": "^6.0|^7.0",
    "kaishiyoku/laravel-html-purifier": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Kaishiyoku\\LaravelMenu\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kaishiyoku\\LaravelMenu\\ServiceProvider"
      ],
      "aliases": {
        "LaravelMenu": "Kaishiyoku\\LaravelMenu\\Facade"
      }
    }
  },
  "minimum-stability": "stable"
}
